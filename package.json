{
  "name": "notionic",
  "version": "3.0.0",
  "homepage": "https://harunonsystem.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/harunonsystem/notionic.git"
  },
  "author": {
    "name": "harunonsystem"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "format": "biome format . --write",
    "format:check": "biome format .",
    "eslint": "eslint --config .eslintrc.cjs components/ pages/ lib/ --ext .js,.jsx,.ts,.tsx",
    "lint": "bun run eslint && biome check components/ pages/ lib/ && biome format components/ pages/ lib/ --write",
    "postbuild": "next-sitemap --config next-sitemap.config.ts",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "check": "bun run lint && bun run typecheck && bun format && bun run test",
    "performance:build": "bun run build && echo 'Build analysis:' && ls -la .next/static/chunks/pages/"
  },
  "dependencies": {
    "feed": "^5.1.0",
    "framer-motion": "^12.23.24",
    "got": "^14.6.0",
    "lucide-react": "^0.546.0",
    "next": "16.0.0",
    "next-themes": "^0.4.6",
    "notion-client": "^7.7.1",
    "notion-types": "^7.7.1",
    "notion-utils": "^7.7.1",
    "nprogress": "^0.2.0",
    "p-map": "^7.0.3",
    "p-memoize": "^8.0.0",
    "prism-theme-vars": "^0.2.5",
    "prismjs": "^1.30.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-notion-x": "^7.7.1",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^15.5.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.8.1",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vercel/analytics": "^1.5.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "8.57.1",
    "eslint-config-next": "^15.5.6",
    "happy-dom": "^20.0.4",
    "lefthook": "^1.13.6",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
